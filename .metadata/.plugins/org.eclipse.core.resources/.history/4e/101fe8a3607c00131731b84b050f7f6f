package featurewindow;

public interface IWindowData
{
	/**
	 * 
	 * @param data
	 * @return if full window
	 */
	public boolean addSamples(float data);

	/**
	 * Set window size
	 * @param size
	 */
	public void setWindowSize(int size);

	/**
	 * Get all data in present window
	 */
	public float[] getData();

}
